pipeline{

	agent any

		stages {
			steps ('scm checkout'){
				git 'https://github.com/vivekdhake/maven-project.git'
				}
			   }
			   
		{
				stage ('compile stage'){
					steps{
					withmaven(maven : 'LocalMaven'){
					sh 'mvn compile'
				    	}
			    	}
		    	} 
					
					stage ('Testing stage'){
					steps{
					withmaven(maven : 'LocalMaven'){
					sh 'mvn test'
						}
					}
				}
				
					stage ('packaging stage'){
					steps{
					withmaven(maven : 'LocalMaven'){
					sh 'mvn package'
						}
					}
				}
				
		}
			
	}		
			
			
